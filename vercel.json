{
  "version": 2,
  "buildCommand": "cd src && npm ci && npm run build",
  "outputDirectory": "output",
  "installCommand": "echo 'Skip root install'",
  "framework": null,
  "rewrites": [
    { "source": "/game", "destination": "/game.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
